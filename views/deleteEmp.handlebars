<table class="table container table-bordered mt-4">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>City</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr>
      <td>{{name}}</td>
      <td>{{email}}</td>
      <td>{{phone}}</td>
      <td>{{city}}</td>
     
      <td class="btn btn-danger">
        <a href="final-delete/{{this._id}}">Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>